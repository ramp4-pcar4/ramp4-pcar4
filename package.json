        "@vue/tsconfig": "^0.1.3",
        "autoprefixer": "^10.4.4",
        "cypress": "^9.5.3",
        "cypress-plugin-tab": "~1.0.5",
        "d3-dsv": "^3.0.1",
        "eslint": "^8.23.1",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-vue": "^9.5.1",
        "postcss": "^8.4.12",
        "postcss-import": "^14.1.0",
        "prettier": "^2.7.1",
        "sass": "^1.50.0",
        "start-server-and-test": "^1.14.0",
        "tailwindcss": "^3.0.23",
        "typedoc": "^0.24.8",
        "typescript": "^4.6.4",
        "vite": "^4.1.4",
